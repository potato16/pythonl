import sys
def isabundant(n):
	tmp=0
	st =1
	se =n
	i=st
	while i>= st and i<=se:
		print (i)
		if n%i==0:
			tmp += i + n//i
			st =i
			se =n//i
		i=i+1
	if tmp >n:
		return True
	else:
		return False
